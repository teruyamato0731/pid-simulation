   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# 時定数\n",
    "tau = 1\n",
    "\n",
    "# 一時遅れ系のシミュレーション\n",
    "t = np.linspace(0, 2*np.e, 100)\n",
    "y = 1 - np.exp(-t/tau)\n",
    "plt.plot(t, y)\n",
    "plt.show()"
   ]
  }
 ],
